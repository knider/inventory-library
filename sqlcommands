//copy and paste to manually perform queries and save time



//create a new borrower
insert into borrower(emailAddress,name,phoneNumber,streetAddress) values(" "," ", " " , " ");

//create a new item
insert into item(features,info,itemName,itemNumber,os,status,type) values(
"features", "info", "itemName", "itemNumber" ,"os", status ,"type");

//create new item_borrower
insert into item_borrower(item_id,borrower_email) values((select id from item where itemNumber = ""),(select emailAddress from borrower where name=""))